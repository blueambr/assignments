{
  "name": "assignments",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "solutions/*",
    "packages/*"
  ],
  "scripts": {
    "solution": "turbo run solution",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "eslint": "^8.33.0",
    "eslint-config-custom": "*",
    "husky": "^8.0.0",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.19.3"
}
